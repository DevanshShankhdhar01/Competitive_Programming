#include <bits/stdc++.h>
using namespace std;

const int MAXN = 200000;      // change according to constraints
const int LOG   = 25;         // since 2^20 > 1e6

vector<int> adj[MAXN+1];
int up[MAXN+1][LOG];          // up[v][j] = 2^j-th ancestor of v
int depth[MAXN+1];

void dfs(int v, int p) {
    up[v][0] = p;

    for(int j = 1; j < LOG; j++)
        up[v][j] = up[ up[v][j-1] ][j-1];

    for(int u : adj[v]) 
        if(u != p) {
            depth[u] = depth[v] + 1;
            dfs(u, v);
        }
}

int lift(int v, int k) {
    for(int j = 0; j < LOG; j++)
        if(k & (1 << j))
            v = up[v][j];
    return v;
}

int lca(int a, int b) {
    if(depth[a] < depth[b])
        swap(a, b);

    // 1) Lift a to same depth as b
    int diff = depth[a] - depth[b];
    a = lift(a, diff);

    if(a == b) return a;

    // 2) Lift both while parent differs
    for(int j = LOG-1; j >= 0; j--)
        if(up[a][j] != up[b][j]) {
            a = up[a][j];
            b = up[b][j];
        }

    // 3) Now parent is LCA
    return up[a][0];
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int n; 
    cin >> n;

    for (int i = 1; i <= n; i++) {
            adj[i].clear();
            for (int j = 0; j < LOG; j++)
                up[i][j] = 0;
            depth[i] = 0;
        }

    for(int i = 1; i < n; i++) {
        int u, v;
        cin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    depth[1] = 0;
    dfs(1, 1);    // root = 1, parent of root = itself

    int q; 
    cin >> q;
    while(q--) {
        int a, b; 
        cin >> a >> b;
        cout << lca(a, b) << "\n";
    }
}
